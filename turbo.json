{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"clean": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"start": {
			"dependsOn": ["^start"],
			"cache": false
		},
		"lint": {
			"dependsOn": ["^lint"]
		}
	}
}
